<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aswanabidin.tubessisfo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HalamanLogin" />

        <activity android:name=".AfterSplash" />
        <activity android:name=".HalamanRegister" />
        <activity android:name=".HalamanBiodata" />
        <activity
            android:name=".DetailView"
            android:label="Detail Kesehatan"
            android:parentActivityName=".HalamanTabbed"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".HalamanTabbed"
            android:label="@string/title_activity_halaman_tabbed"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DetailAnggota"
            android:label="Detail Anggota"
            android:parentActivityName=".HalamanTabbed">

        </activity>
    </application>

</manifest>