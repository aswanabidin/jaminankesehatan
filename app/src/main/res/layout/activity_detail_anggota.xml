<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.aswanabidin.tubessisfo.DetailAnggota">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="24dp"
        android:gravity="center"
        android:text="Data Pengguna Jaminan Kesehatan"
        android:textColor="@color/Black"
        android:textSize="15dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textViewNomorKartuKeluarga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView7"
        android:layout_marginTop="30dp"
        android:layout_toEndOf="@+id/textView6"
        android:text="textview"
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textViewNomorRekening"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textViewNomorKartuKeluarga"
        android:layout_below="@+id/textViewNomorKartuKeluarga"
        android:layout_marginTop="20dp"
        android:text="textview"
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textViewNomorKartuKeluarga"
        android:layout_alignBottom="@+id/textViewNomorKartuKeluarga"
        android:layout_alignParentStart="true"
        android:layout_marginStart="26dp"
        android:text="Nomor Kartu Keluarga  : "
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textViewNomorRekening"
        android:layout_alignBottom="@+id/textViewNomorRekening"
        android:layout_alignStart="@+id/textView2"
        android:text="Nomor Rekening            : "
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView4"
        android:layout_below="@+id/textView4"
        android:layout_marginTop="18dp"
        android:text="Nama Lengkap               : "
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView5"
        android:layout_below="@+id/textView5"
        android:layout_marginTop="17dp"
        android:text="Tempat Lahir                  : "
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView6"
        android:layout_below="@+id/textView6"
        android:layout_marginTop="16dp"
        android:text="Tanggal Lahir                  : "
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView8"
        android:layout_centerVertical="true"
        android:text="Alamat Rumah               : "
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView9"
        android:layout_below="@+id/textView9"
        android:layout_marginTop="18dp"
        android:text="Jenis Kelamin                 : "
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView10"
        android:layout_below="@+id/textView10"
        android:layout_marginTop="21dp"
        android:text="Status Pernikahan         : "
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView11"
        android:layout_below="@+id/textView11"
        android:layout_marginTop="25dp"
        android:text="Nomor Telepon              : "
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView11"
        android:layout_below="@+id/textView12"
        android:layout_marginTop="25dp"
        android:text="Alamat Email                  : "
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView11"
        android:layout_below="@+id/textView13"
        android:layout_marginTop="25dp"
        android:text="Fasilitas Kesehatan       : "
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textViewNamaLengkap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView5"
        android:layout_toEndOf="@+id/textView6"
        android:text="TextView"
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textViewTempatLahir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView6"
        android:layout_toEndOf="@+id/textView6"
        android:text="TextView"
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textViewTanggalLahir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView8"
        android:layout_alignBottom="@+id/textView8"
        android:layout_toEndOf="@+id/textView6"
        android:text="TextView"
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textViewAlamatRumah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView10"
        android:layout_toEndOf="@+id/textView8"
        android:text="TextView"
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textViewJenisKelamin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView10"
        android:layout_alignBottom="@+id/textView10"
        android:layout_alignStart="@+id/textViewAlamatRumah"
        android:text="TextView"
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textViewStatusPernikahan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView12"
        android:layout_toEndOf="@+id/textView12"
        android:text="TextView"
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textViewNomorTelepon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView12"
        android:layout_alignBottom="@+id/textView12"
        android:layout_alignStart="@+id/textViewFasilitasKesehatan"
        android:text="TextView"
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textViewAlamatEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView13"
        android:layout_alignBottom="@+id/textView13"
        android:layout_toEndOf="@+id/textView12"
        android:text="TextView"
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textViewFasilitasKesehatan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView14"
        android:layout_alignBottom="@+id/textView14"
        android:layout_toEndOf="@+id/textView8"
        android:text="TextView"
        android:textColor="@color/Black" />

</RelativeLayout>
